{
  "name": "emojis-trainer",
  "version": "0.5.0",
  "description": "Build an emojis sprite and stylesheet file from emojis datas collected from both emojipedia.org and unicode.org website",
  "scripts": {
    "start": "node ./dist/index.js",
    "transpile": "$(npm bin)/babel . --out-dir dist --ignore cache,tests,node_modules,nyc-reports,dist",
    "prepublish": "yarn run test",
    "test": "nyc --check-coverage --report-dir=${CIRCLE_ARTIFACTS:-'./nyc-reports'} --reporter=lcov yarn run mocha",
    "mocha": "yarn run transpile && mocha --compilers js:babel-register -r tests/bootstrap.js $(find tests -name '*.test.js')"
  },
  "bin": {
    "emojis-trainer": "./bin/emojis-trainer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/arthur-feral/emojis-sprites-generator.git"
  },
  "engines": {
    "node": ">=7.1.0"
  },
  "bugs": {
    "url": "https://github.com/arthur-feral/emojis-sprites-generator/issues"
  },
  "homepage": "https://github.com/arthur-feral/emojis-sprites-generator#readme",
  "keywords": [
    "emoji",
    "emojis",
    "generator",
    "sprite",
    "sprites",
    "svg",
    "png",
    "jpeg",
    "jpg"
  ],
  "author": "Arthur FÃ©ral <arthur.feral@hotmail.fr>",
  "license": "ISC",
  "dependencies": {
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "commander": "^2.9.0",
    "fs-extra": "^1.0.0",
    "globby": "^6.1.0",
    "gm": "^1.23.0",
    "image-size": "^0.5.0",
    "jimp": "^0.2.27",
    "lodash": "^4.17.0",
    "superagent": "^2.3.0",
    "superagent-throttle": "^0.2.1",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "mock-fs": "^3.12.1",
    "nyc": "^11.3.0",
    "sinon": "^1.17.6",
    "superagent-mock": "^2.0.1"
  },
  "nyc": {
    "exclude": [
      "dist/"
    ]
  }
}
